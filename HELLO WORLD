a = 0
b = 1 
l = ["h","e", "l","l", "o","w","o","r","l","d"]
k = [] 
if a+b==1 :
    print("Hello Wooooooooooooooorld")
else : 
  print("hehe")
#HELLO
for i in range(len(l)):
    i = i+1
    print(l[i-1])

for i in range(len(l)):
    i = i+1
    k.append(l[i-1])
    print(k)
#WORLD
for i in range(len(l)):
    i = i+1
    print(l[:i])
i=0

while i < 15 :
    i=i+1
    print("hello who ? HellooWWWorrrrlldd")
